{
  "compilerOptions": {
    "esModuleInterop": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "skipLibCheck": true,
    "sourceMap": false,
    "resolveJsonModule": true,
    "jsx": "preserve",
    "lib": [
      "es6",
      "esnext.asynciterable",
      "dom"
    ],
    "target": "es5",
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "noEmit": true,
    "baseUrl": "./src",
    "paths": {
      "#/*": [
        "./*"
      ]
    },
    "plugins": [
      {
        "transform": "typescript-transform-paths"
      }
    ],
    "allowJs": true,
    "incremental": true
  },
  "include": [
    "./next-env.d.ts",
    "jest.config.ts",
    "./src"
  ],
  "exclude": [
    "node_modules"
  ]
}
