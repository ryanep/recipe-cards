{
  "name": "Run application",
  "appPort": ["3000:3000"],
  "build": {
    "dockerfile": "./dockerfile",
    "args": { "VARIANT": "20" }
  },
  "postCreateCommand": "npm install",
  "remoteUser": "node",
  "features": {
    "git": "latest"
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "bradlc.vscode-tailwindcss",
        "dbaeumer.vscode-eslint",
        "editorconfig.editorconfig",
        "esbenp.prettier-vscode",
        "prisma.prisma",
        "qwtel.sqlite-viewer",
        "waderyan.gitblame"
      ]
    }
  }
}
