{
  "name": "Run application",
  "appPort": ["3000:3000"],
  "build": {
    "dockerfile": "./dockerfile"
  },
  "postCreateCommand": "npm install",
  "remoteUser": "node",
  "features": {
    "git": "latest",
    "node": "20.12.0"
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "bradlc.vscode-tailwindcss",
        "dbaeumer.vscode-eslint",
        "editorconfig.editorconfig",
        "esbenp.prettier-vscode",
        "prisma.prisma",
        "waderyan.gitblame",
        "yy0931.vscode-sqlite3-editor"
      ]
    }
  }
}
